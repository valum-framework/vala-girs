      <method name="set_rate" c:identifier="totem_object_set_rate">
        <doc xml:space="preserve">Sets the playback rate, with `1.0` being the normal playback rate.</doc>
        <return-value transfer-ownership="none">
          <doc xml:space="preserve">%TRUE on success, %FALSE on failure.</doc>
          <type name="gboolean" c:type="gboolean"/>
        </return-value>
