    <function name="interface_create_header_button"
              c:identifier="totem_interface_create_header_button">
      <doc xml:space="preserve">Put the given @icon_name into @button, and pack @button into @header
according to @pack_type.</doc>
      <return-value transfer-ownership="none">
        <doc xml:space="preserve">the button passed as input</doc>
        <type name="Gtk.Widget" c:type="GtkWidget*"/>
      </return-value>
      <parameters>
        <parameter name="header" transfer-ownership="none">
          <doc xml:space="preserve">The header widget to put the button in</doc>
          <type name="Gtk.Widget" c:type="GtkWidget*"/>
        </parameter>
        <parameter name="button" transfer-ownership="none">
          <doc xml:space="preserve">The button to use in the header</doc>
          <type name="Gtk.Widget" c:type="GtkWidget*"/>
        </parameter>
        <parameter name="icon_name" transfer-ownership="none">
          <doc xml:space="preserve">The icon name for the button image</doc>
          <type name="utf8" c:type="const char*"/>
        </parameter>
        <parameter name="pack_type" transfer-ownership="none">
          <doc xml:space="preserve">A #GtkPackType to tell us where to include the button</doc>
          <type name="Gtk.PackType" c:type="GtkPackType"/>
        </parameter>
      </parameters>
