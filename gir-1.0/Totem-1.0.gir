      <method name="seek_previous" c:identifier="totem_object_seek_previous">
        <doc xml:space="preserve">If a DVD is being played, goes to the previous chapter. If a normal stream
is being played, goes to the start of the stream if possible. If seeking is
not possible, plays the previous entry in the playlist.</doc>
        <return-value transfer-ownership="none">
          <type name="none" c:type="void"/>
        </return-value>
        <parameters>
          <instance-parameter name="totem" transfer-ownership="none">
            <doc xml:space="preserve">a #TotemObject</doc>
            <type name="Object" c:type="TotemObject*"/>
          </instance-parameter>
        </parameters>
      </method>
      <method name="seek_relative" c:identifier="totem_object_seek_relative">
        <doc xml:space="preserve">Seeks to an @offset from the current position in the stream,
or displays an error dialog if that's not possible.</doc>
        <return-value transfer-ownership="none">
          <type name="none" c:type="void"/>
        </return-value>
        <parameters>
          <instance-parameter name="totem" transfer-ownership="none">
            <doc xml:space="preserve">a #TotemObject</doc>
            <type name="Object" c:type="TotemObject*"/>
